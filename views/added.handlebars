<div class="row">
  <div class="col-md-12">
    <h2>Lock Saved </h2>
     {{#each locks}}
    <div class="form added-form">
        <div class="form-group">
        <p for="siteName">Site:    {{this.site}}</p>
                                   
      </div>
      <div class="form-group">
        <p for="username">Username:  {{this.username}} </p>
        
      </div>
      <div class="form-group">
        <p for="password">Password:   {{this.password}} </p>
        
      </div>
      <div class="col-md-4">
        <button class="btn btn-sm btn-danger" data-id="{{this.id}}">DELETE</button>
      </div>
      
    </div>
    <br>
    {{/each}}
  </div>

</div>

<script src="./js/added.js"></script>